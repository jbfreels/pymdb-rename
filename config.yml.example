action: copy                      # copy, move
movie_format: "tests/data/output/{n} ({y})"         # The Matrix (1999)
movie_exts:
  - ".avi"
  - ".mp4"
  - ".m4v"
  - ".mkv"
logger:
  file: 
    level: logging.DEBUG
    format: "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    path: pymdb-rename.log
  stdout:
    level: logging.ERROR
    format: "%(levelname)s: %(name)s - %(message)s"
